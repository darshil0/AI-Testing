# API Keys for AI Models
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key
OPENAI_API_KEY=

# Anthropic API Key
ANTHROPIC_API_KEY=

# Google AI API Key
GOOGLE_API_KEY=

# Evaluation Settings
MAX_TOKENS=2000
TEMPERATURE=0.7
TIMEOUT_SECONDS=60

# Judge Settings
# Allowed values: openai, anthropic, gemini
JUDGE_MODEL=openai
# Example: gpt-4o, gpt-4.1, claude-3-opus, gemini-1.5-pro
JUDGE_SPECIFIC_MODEL=gpt-4o

# Result Storage
RESULTS_DIR=ai_evaluation/results
TEST_CASES_DIR=ai_evaluation/test_cases

# Logging
LOG_LEVEL=INFO
LOG_FILE=evaluation.log
