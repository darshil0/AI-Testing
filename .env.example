# API Keys for AI Models
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI API Key
GOOGLE_API_KEY=your_google_api_key_here

# Evaluation Settings
MAX_TOKENS=2000
TEMPERATURE=0.7
TIMEOUT_SECONDS=60

# Judge Settings
JUDGE_MODEL=openai  # openai, anthropic, or gemini
JUDGE_SPECIFIC_MODEL=gpt-4o  # specifically which model to judge with

# Result Storage
RESULTS_DIR=ai_evaluation/results
TEST_CASES_DIR=ai_evaluation/test_cases

# Logging
LOG_LEVEL=INFO
LOG_FILE=evaluation.log
